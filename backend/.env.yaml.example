env_variables:
  NODE_ENV: "production"
  
  # Database Configuration
  # Option 1: Cloud SQL (recommended)
  # DB_HOST: "/cloudsql/mobiuschatgpt:us-central1:financialmanager-db"
  # Option 2: External database
  DB_HOST: "your-database-host.com"
  DB_USER: "postgres"
  DB_PASSWORD: "your_secure_password_here"
  DB_PORT: "5432"
  DB_NAME: "financialmanager"
  
  # Google OAuth Configuration
  # Get from: https://console.cloud.google.com/apis/credentials?project=mobiuschatgpt
  GOOGLE_CLIENT_ID: "your_google_client_id.apps.googleusercontent.com"
  
  # JWT Secret - Generate with: openssl rand -base64 32
  JWT_SECRET: "your_super_secret_jwt_key_change_this"
  
  # Auth Bypass - MUST be "false" in production
  AUTH_BYPASS: "false"
  
  # Server Port (App Engine uses PORT environment variable)
  PORT: "8080"

